    "@mui/material": "^5.2.4",
    "@mui/styles": "^5.2.3",
    "@reduxjs/toolkit": "^1.7.1",
    "@supabase/supabase-js": "^1.28.5",
    "axios": "^0.24.0",
    "country-flag-icons": "^1.4.19",
    "d3-array": "^3.1.1",
    "d3-ease": "^3.0.1",
    "date-fns": "^2.27.0",
    "i18next": "^21.6.0",
    "i18next-browser-languagedetector": "^6.1.2",
    "igdb-api-types": "^0.1.0",
    "immer": "^9.0.7",
    "js-cookie": "^3.0.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "next": "^12.0.7",
    "next-seo": "^4.28.1",
    "notistack": "^2.0.3",
    "ramda": "^0.27.1",
    "react": "^17.0.2",
    "react-confetti": "^6.0.1",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.22.1",
    "react-i18next": "^11.15.1",
    "react-redux": "^7.2.6",
