{
  "name": "honey-votes",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "chat-goal-widget"
  ],
  "scripts": {
    "dev": "next dev -p 3001",
    "prebuild": "yarn build:chat-goal-widget",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "build:chat-goal-widget": "rm -rf ./public/chat-goal-widget && cd ./chat-goal-widget && yarn build && yarn export -o ../public/chat-goal-widget",
    "generate:types": "npx openapi-typescript http://localhost:3000/api-json --output src/features/api/apiTypes.generated.ts",
    "generate:kinopoisk-types": "npx openapi-typescript https://kinopoiskapiunofficial.tech/documentation/api/openapi.json --output src/features/kinopoisk-api/kinopoiskApiTypes.generated.ts",
    "generate:schema": "node ./scripts/generateApiSchema.js http://localhost:3000/api-json src/features/api/apiSchema.json",
    "gen": "yarn generate:types && yarn generate:schema && yarn generate:kinopoisk-types"
  },
  "dependencies": {
    "@emotion/react": "^11.6.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.1.0",
    "@mui/material": "^5.1.0",
    "@mui/styles": "^5.1.0",
    "@reduxjs/toolkit": "^1.6.2",
    "@supabase/supabase-js": "^1.28.0",
    "axios": "^0.24.0",
    "d3-array": "^3.1.1",
    "d3-ease": "^3.0.1",
    "date-fns": "^2.25.0",
    "immer": "^9.0.6",
    "js-cookie": "^3.0.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "next": "^12.0.4",
    "next-seo": "^4.28.1",
    "ramda": "^0.27.1",
    "react": "^17.0.2",
    "react-confetti": "^6.0.1",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.19.5",
    "react-redux": "^7.2.6",
    "react-spinning-canvas-wheel": "^0.1.4",
    "react-use": "^17.3.1",
    "use-sound": "^4.0.1"
  },
  "devDependencies": {
    "@types/d3-array": "^3.0.2",
    "@types/gtag.js": "^0.0.7",
    "@types/js-cookie": "^3.0.1",
    "@types/lodash": "^4.14.177",
    "@types/node-fetch": "^3.0.3",
    "@types/ramda": "^0.27.49",
    "cross-env": "^7.0.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
